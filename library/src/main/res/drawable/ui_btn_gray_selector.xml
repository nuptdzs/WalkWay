<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <itemn android:state_pressed="false" android:state_enabled="true">
        <shape android:shape="rectangle">
            <stroke android:width="1dip" android:color="@color/color_white_8c8fa3" />
            <solid android:color="@color/transparent_background" />
            <corners android:radius="5dp" />
        </shape>
    </itemn>

    <item android:state_pressed="false" android:state_enabled="false">
        <shape android:shape="rectangle">
            <stroke android:width="1dip" android:color="@color/color_white_8c8fa3" />
            <solid android:color="@color/transparent_background" />
            <corners android:radius="5dp" />
        </shape>
    </item>

    <item android:state_pressed="true" android:state_enabled="true">
        <shape android:shape="rectangle">
            <stroke android:width="1dip" android:color="@color/color_white_8c8fa3" />
            <solid android:color="@color/transparent_background" />
            <corners android:radius="5dp" />
        </shape>
    </item>
</selector>