<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="true" android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="#f9fdff" />
            <corners android:radius="5dp" />
            <stroke android:width="1dp" android:height="1dp" android:color="#59c1fb" />
        </shape>
    </item>

    <item android:state_enabled="false" android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="#f9fdff" />
            <corners android:radius="5dp" />
            <stroke android:width="1dp" android:height="1dp" android:color="#c7ccce" />
        </shape>
    </item>

    <item android:state_enabled="true" android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#f9fdff" />
            <corners android:radius="5dp" />
            <stroke android:width="1dp" android:height="1dp" android:color="#c7ccce" />
        </shape>
    </item>
</selector>